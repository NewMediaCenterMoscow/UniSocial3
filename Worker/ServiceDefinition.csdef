<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="Worker" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2013-10.2.2">
  <WorkerRole name="WorkerRole" vmsize="Small">
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="StorageConnectionString" />
      <Setting name="tasksQueueName" />
      <Setting name="resultsQueueName" />
      <Setting name="resultQueueContainerName" />
    </ConfigurationSettings>
  </WorkerRole>
  <WorkerRole name="DbWriterRole" vmsize="Small">
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="StorageConnectionString" />
      <Setting name="resultsQueueName" />
      <Setting name="tasksQueueName" />
      <Setting name="resultQueueContainerName" />
    </ConfigurationSettings>
  </WorkerRole>
</ServiceDefinition>